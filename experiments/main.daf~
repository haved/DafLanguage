#import "base.daf"

def N:=4
def PI:=3.14159265

def main:=() {
	bolle(4);
}

def bolle:=(x:int) {
	let num:=5;
	let mut tall:= {
		let mut x:= 4;
		x++;
		x++;
		x 
	}
		
}

def sub:=(x:int, y:int) {
	x-y
}

struct Circle {
	radius:int
}

impl Circle {
	def area:=(&self:Circle) {
		self.radius * self.radius * PI 
	}

	def circumference:=(&self:Circle) {
		self.radius * 2 * PI
	}

	def new:=(radius:int)->Circle {
		let c:Circle;
		c.radius=radius;
		c
	}	
}
